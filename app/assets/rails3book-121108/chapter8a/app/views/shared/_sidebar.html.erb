<% unless @current_member %>
<%= render "shared/login_form" %>
<% end %>

<h2>最新ニュース</h2>
<ul>
<% Article.sidebar_articles(@current_member, 5).each do |article| %>
<li><%= link_to article.title, article %></li>
<% end %>
</ul>
<h2>会員のブログ</h2>
<ul>
<% 5.times do |i| %>
<li><%= link_to "ブログの見出し", "#" %></li>
<% end %>
</ul>
